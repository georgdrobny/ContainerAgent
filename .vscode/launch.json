    "version": "0.2.0",
    "configurations": [
        {
            "name": "DownloadAgent-Custom",
            "type": "PowerShell",
            "request": "launch",
            "script": "${workspaceFolder}/Common/DownloadAgent.ps1",
            "args": ["-AzureDevopsOrganization $env:ADOORG -AzureDevopsPersonalAccessToken $ENV:ADOPAT -DestinationDirectory C:\\Temp\\Agent -CustomAgentLocation https://vstsagentpackage.azureedge.net/agent/2.214.1/vsts-agent-win-x64-2.214.1.zip"]
        },
        {
            "name": "DownloadAgent-Latest",
            "type": "PowerShell",
            "request": "launch",
            "script": "${workspaceFolder}/Common/DownloadAgent.ps1",
            "args": ["-AzureDevopsOrganization $env:ADOORG -AzureDevopsPersonalAccessToken $ENV:ADOPAT -DestinationDirectory C:\\Temp\\Agent"]
        }
    ]
}